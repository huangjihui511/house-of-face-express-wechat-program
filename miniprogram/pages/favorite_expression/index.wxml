<!--index.wxml-->
<view class="button_list">
  <button class='time' style="width:250rpx" bindtap="time_order">收藏时间</button>
  <button class='freq' style="width:250rpx" bindtap="freq_order">使用频率</button>
  <picker style="width:250rpx" class='classify' bindchange="bindPickerChange" value="{{index}}" range="{{classes}}">
    <view class="picker">          
      选择类别：{{classes[index]}}        
    </view>      
  </picker>
</view>
<view class="divLine"></view>
<text>\n</text>
<view class="struct">
  <view wx:for="{{images}}" wx:key="{{index}}">
    <view class="image_struct">
      <image class="image" src="{{item.image_address}}" bindlongtap="previewImage"></image>
      <view class="image_title">{{item.image_name}}</view>
    </view>
  </view>
</view>