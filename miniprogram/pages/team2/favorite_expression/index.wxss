/**index.wxss**/

page {
  background:white;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.delete_logo{
  width: 50rpx;
  height: 50rpx;
}
.delete{
  position: fixed;
  bottom: 50rpx;
  height: 70rpx;
  width: 100%;
  background-color: rgb(252, 251, 251);
  color: rgb(76, 196, 126);
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.image{
  height: 160rpx;
  width: 160rpx;
}
.load_image_login{
  height: 160rpx;
  width: 160rpx;
  margin: 30rpx 30rpx 400rpx 30rpx;
}
.temp{
  margin: 30rpx 30rpx 400rpx 30rpx;
}
.empty{
  display: none;
}
.image_selected{
  border: 3px double rgb(76, 196, 126);
}
.struct{
  display: flex;
  flex-wrap:wrap;
  justify-content:space-around;
}
.image_struct{
  flex-direction: column;
  flex-wrap:wrap;
  justify-content:space-around;
  margin: 30rpx;
}
 .select{
  background-color: rgb(252, 251, 251);
   display: flex;
   flex-direction:row;
   align-items: baseline;
 }
 .all_select{
  background-color: rgb(252, 251, 251);
  color: rgb(76, 196, 126);
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  font-size: 30rpx;
  text-align: center;
  height: 60rpx;
  width: 100rpx;
 }
 .select_number{
  background-color: rgb(252, 251, 251);
  position: fixed;
  bottom: 0rpx;
  left: 100rpx;
  font-size: 40rpx;
  height: 60rpx;
  width: 560rpx;
  text-align: center;
  color: rgb(100, 97, 97);
 }
 .select_text{
   color: rgb(76, 196, 126);
   background-color: rgb(252, 251, 251);
  position: fixed;
  bottom: 0rpx;
  right: 0rpx;
  font-size: 30rpx;
  text-align: center;
  height: 60rpx;
  width: 100rpx;
 }

.nav {
  height: 80rpx;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 80rpx;
  background: rgb(252, 251, 251);
  font-size: 16px;
  white-space: nowrap;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99;
}
.nav-item {
  width: 20%;
  display: inline-block;
  text-align: center;
}
.nav-item.active{
  color: rgb(76, 196, 126);
}
.page__title {
  width: 750rpx;
  text-align: center;
  font-size: 20px;
  font-weight: 400;
}

.page__desc {
  margin-top: 5px;
  color: #888888;
  text-align: left;
  font-size: 14px;
}

/* pages/login/login.wxss */

page{
  background-color: #F8F8F8;
  font-size: 16px;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.page__hd {
  padding: 40px;
}
.page__bd {
  padding-bottom: 40px;
}
.page__bd_spacing {
  padding-left: 15px;
  padding-right: 15px;
}

.page__ft{
  padding-bottom: 10px;
  text-align: center;
}

.page__title {
  text-align: left;
  font-size: 20px;
  font-weight: 400;
}

.page__desc {
  margin-top: 5px;
  color: #888888;
  text-align: left;
  font-size: 14px;
}
.weui-cell_example:before{
  left:52px;
}

.button-sp-area{
  margin: 0 auto;
  width: 60%;
}
.mini-btn{
  margin-right: 10rpx;
}


.page-body {
  width: 100%;
  flex-grow: 1;
  overflow-x: hidden;
}
.page-body-wrapper {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}
.page-body-wording {
  text-align: center;
  padding: 200rpx 100rpx;
}
.page-body-info {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #fff;
  width: 100%;
  padding: 50rpx 0 150rpx 0;
}
.page-body-title {
  margin-bottom: 100rpx;
  font-size: 32rpx;
}
.page-body-text {
  font-size: 30rpx;
  line-height: 26px;
  color: #ccc;
}
.page-body-text-small {
  font-size: 24rpx;
  color: #000;
  margin-bottom: 100rpx;
}

.page-foot{
  margin: 100rpx 0 30rpx 0;
  text-align: center;
  color: #1aad19;
  font-size: 0;
}