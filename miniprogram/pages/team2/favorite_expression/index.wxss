/**index.wxss**/

page {
  background:white;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.all{
  
  display: flex;
  flex-direction: column;
}
.select{
  position: fixed;
  top: 0px;
  right: 0px;
}
.image{
  height: 160rpx;
  width: 160rpx;
}
.struct{
  display: flex;
  flex-wrap:wrap;
  justify-content:space-around;

}
.image_struct{
  flex-direction: column;
  flex-wrap:wrap;
  justify-content:space-around;
  margin: 30rpx;
}
.image_title{
  text-align:center;
  font-weight:700;
}
.button_list{

  display: flex;
  flex-wrap:wrap;
  align-items:baseline;
}
.freq{
  background-color:transparent;
  color: black;
  font-size: 30rpx;
}
.freq:hover {
  background-color: orange;
  color: white;
}
.picker{
  color: black;
  font-weight:600;
  font-size: 30rpx;
}
.divLine{
  position: fixed;
  background: black;
  width: 100%;
  height: 5rpx;
 }
 .classify{
   text-align: center;
 }